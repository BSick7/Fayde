<Page
       xmlns="WickedSick.Server.XamlParser;WickedSick.Server.XamlParser.Elements"
       xmlns:x="WickedSick.Server.XamlParser;WickedSick.Server.XamlParser.Core"
       xmlns:sys="WickedSick.Server.XamlParser;WickedSick.Server.XamlParser.Core"
       xmlns:vsm="WickedSick.Server.XamlParser;WickedSick.Server.XamlParser.Elements">
    <Grid Background="#FFFFFF">
        <Grid.RowDefinitions>
            <RowDefinition Height="400" />
            <RowDefinition Height="400" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400" />
            <ColumnDefinition Width="400" />
        </Grid.ColumnDefinitions>
        <Grid>
            <Grid.Background>
                <ImageBrush Stretch="None" ImageSource="tile-test.png" />
            </Grid.Background>
        </Grid>
        <Grid Grid.Column="1">
            <Grid.Background>
                <ImageBrush Stretch="Fill" ImageSource="tile-test.png" />
            </Grid.Background>
        </Grid>
        <Grid Grid.Row="1">
            <Grid.Background>
                <ImageBrush Stretch="Uniform" ImageSource="tile-test.png" />
            </Grid.Background>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="1">
            <Grid.Background>
                <ImageBrush Stretch="UniformToFill" ImageSource="tile-test.png" />
            </Grid.Background>
        </Grid>
    </Grid>
</Page>