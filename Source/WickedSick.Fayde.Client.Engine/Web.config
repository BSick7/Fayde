<?xml version="1.0"?>
<configuration>
    <system.webServer>
        <modules runAllManagedModulesForAllRequests="true">
            <add name="FaydeModule" type="WickedSick.Server.Framework.Fayde.FaydeHttpModule"/>
        </modules>
        <handlers>
            <add verb="*" path="*.fayde" name="FaydeHttpHandler" type="WickedSick.Server.Framework.Fayde.FaydeHttpHandler"/>
        </handlers>
    </system.webServer>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
    </system.web>
</configuration>
